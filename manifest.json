{
  "name": "Auto Rewards",
  "description": "Rewards please!",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["tabs", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "https://*.americanexpress.com/offers/eligible*",
    "https://*.chase.com/*",
  ],
  "action": {
    "default_popup": "popup.html"
  }
}